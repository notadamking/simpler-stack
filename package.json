{
  "name": "GREEN-stack-example",
  "description": "Example of a webapp using the GREEN stack: a highly efficient, fullstack javascript framework that utilizes Graphql, REact, REdux, REthinkdb, Express, and Node.js.",
  "author": "Adam King <adamjking3@gmail.com> (http://github.com/adamjking3)",
  "license": "MIT",
  "version": "0.9.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/adamjking3/GREEN"
  },
  "homepage": "https://github.com/adamjking3/GREEN",
  "keywords": [
    "graphql",
    "react",
    "redux",
    "rethinkdb",
    "express",
    "node",
    "isomorphic",
    "universal",
    "modern",
    "realtime",
    "starter",
    "boilerplate",
    "ES7"
  ],
  "main": "bin/web-server.js",
  "scripts": {
    "start": "concurrent --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
    "start-prod": "better-npm-run start-prod",
    "start-prod-api": "better-npm-run start-prod-api",
    "build": "better-npm-run build",
    "postinstall": "npm run build",
    "lint": "eslint -c .eslintrc src api",
    "start-dev": "better-npm-run start-dev",
    "start-dev-api": "better-npm-run start-dev-api",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-dev-api\"",
    "test": "karma start",
    "test-node": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register",
    "test-node-watch": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register --watch"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/web-server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3030
      }
    },
    "start-prod-api": {
      "command": "node ./bin/api-server.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "production",
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "node ./bin/web-server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "start-dev-api": {
      "command": "node ./bin/api-server.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "APIPORT": 3030
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "aphrodite": "^0.4.0",
    "apollo-client": "^0.4.1",
    "apollo-server": "^0.1.5",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "bcryptjs": "^2.3.0",
    "classnames": "^2.2.5",
    "compression": "^1.6.0",
    "concurrently": "^2.2.0",
    "express": "^4.13.3",
    "file-loader": "^0.8.5",
    "graphql": "^0.6.0",
    "graphql-errors": "^2.1.0",
    "http-proxy": "^1.12.0",
    "joi": "^9.0.1",
    "jsonwebtoken": "^7.1.6",
    "piping": "^0.3.0",
    "pretty-error": "^1.2.0",
    "react": "^0.14.8",
    "react-apollo": "^0.3.19",
    "react-dom": "^0.14.8",
    "react-dom-stream": "^0.5.1",
    "react-helmet": "^2.2.0",
    "react-redux": "^4.0.0",
    "react-router": "2.0.0",
    "react-router-redux": "^4.0.0",
    "react-semantify": "^0.5.1",
    "redux": "^3.0.4",
    "redux-connect": "^2.4.0",
    "redux-form": "^3.0.12",
    "redux-logger": "^2.6.1",
    "rethinkdb": "^2.3.2",
    "scroll-behavior": "^0.3.2",
    "serialize-javascript": "^1.1.2",
    "serve-favicon": "^2.3.0",
    "socket.io": "^1.3.7",
    "socket.io-client": "^1.3.7",
    "superagent": "^1.4.0",
    "thinky": "^2.3.2",
    "url-loader": "^0.5.7",
    "validator": "^5.4.0",
    "warning": "^2.1.0",
    "webpack-isomorphic-tools": "^2.2.18"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
    "babel-plugin-typecheck": "^3.9.0",
    "better-npm-run": "0.0.8",
    "chai": "^3.3.0",
    "clean-webpack-plugin": "^0.1.6",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "0.1.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-import": "^0.8.0",
    "eslint-plugin-react": "^3.5.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "graphiql": "^0.7.3",
    "json-loader": "^0.5.4",
    "karma": "^0.13.10",
    "karma-cli": "^0.1.1",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.3",
    "phantomjs": "^1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "react-addons-test-utils": "^0.14.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "strip-loader": "^0.1.0",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.5.0"
  },
  "engines": {
    "node": "5.6.0"
  }
}
